@model IEnumerable<SportClubs1.Data.TrainingMachine>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Cost)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.State)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.GymAddressNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Cost)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.State)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.GymAddressNavigation.Address)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.InventNum">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.InventNum">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.InventNum">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
